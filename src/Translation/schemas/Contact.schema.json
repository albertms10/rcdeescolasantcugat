    "or-send-email-to": {
      "type": "string",
      "pattern": "^[A-Z].*(?<![.])$"
    },
    "report-error": {
      "type": "string",
      "pattern": "^[A-Z].*(?<![.])$"
    },
    "res-ok": {
      "type": "string",
      "pattern": "^[A-Z].*[.]$"
    },
    "res-invalid": {
      "type": "string",
      "pattern": "^[A-Z].*[.]$"
    },
    "contains-link": {
      "type": "string",
      "pattern": "^[A-Z].*[.]$"
    },
    "empty-err-msg": {
      "type": "string",
      "pattern": "^[A-Z].*[.]$"
    },
    "res-err": {
      "type": "string",
      "pattern": "^[A-Z].*[.]$"
    },
    "res-err-sent": {
      "type": "string",
      "pattern": "^[A-Z].*[.]$"
    }
