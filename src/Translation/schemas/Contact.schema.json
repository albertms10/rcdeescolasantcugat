{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "title": "Contact Translations",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "description": {
      "type": "string",
      "pattern": "^[A-Z].*[.]$"
    },
    "contact-us": {
      "type": "string",
      "pattern": "^[A-Z].*(?<![.])$"
    },
    "name": {
      "type": "string",
      "pattern": "^[A-Z].*(?<![.])$"
    },
    "enter-name": {
      "type": "string",
      "pattern": "^[A-Z].*[.]$"
    },
    "email": {
      "type": "string",
      "pattern": "^[A-Z].*(?<![.])$"
    },
    "enter-email": {
      "type": "string",
      "pattern": "^[A-Z].*[.]$"
    },
    "valid-email": {
      "type": "string",
      "pattern": "^[A-Z].*[.]$"
    },
    "message": {
      "type": "string",
      "pattern": "^[A-Z].*(?<![.])$"
    },
    "enter-message": {
      "type": "string",
      "pattern": "^[A-Z].*[.]$"
    },
    "send": {
      "type": "string",
      "pattern": "^[A-Z].*(?<![.])$"
    },
    "sending": {
      "type": "string",
      "pattern": "^[A-Z].*(?<![.])$"
