{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "title": "Escola Translations",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "description": {
      "type": "string",
      "pattern": "^[A-Z].*[.]$"
    },
    "staff": {
      "type": "string",
      "pattern": "^[A-Z].*(?<![.])$"
    },
    "assistant": {
      "type": "string",
      "pattern": "^[A-Z].*(?<![.])$"
    },
    "coach": {
      "type": "string",
      "pattern": "^[A-Z].*(?<![.])$"
    },
    "technical-director": {
      "type": "string",
      "pattern": "^[A-Z].*(?<![.])$"
    },
    "season": {
      "type": "string",
      "pattern": "^%s.*(?<![.])$"
    },
    "press-clippings": {
      "type": "string",
      "pattern": "^[A-Z].*(?<![.])$"
    },
    "visit-new-window-or-tab": {
      "type": "string",
      "pattern": "^[A-Z].*%s.*(?<![.])$"
    }
  }
}
